  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "module":"nodenext",
    "target": "esnext",
    "lib": ["esnext"],
    "types": ["node"],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts", "prisma.config.ts", "prisma"],
  "exclude": ["node_modules"]
}